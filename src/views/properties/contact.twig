{# extend the parent tpl #}
{% extends 'layout.twig' %}


{# set the pagetitle in the parent tpl #}
{% set pageTitle = 'Struttura' %}


{# set the css file in the parent tpl #}
{% set cssFile = 'contact' %}


{# overwrite the pageContent block #}
{% block pageContent %}
	<div id="loginContent">
            <h2>{{ title }}</h2>
		
            <form action="{#{{ app.request.baseUrl }}/broker/overview#}" method="get" {{ form_enctype(contactform) }} novalidate="novalidate">{# disable HTML5 formchecking #}
                    {{ form_errors(contactform) }}
                    {{ form_widget(contactform) }}
                    <input type="submit" id="btnOk" name="submit" value="Send"/>
            </form>
        </div>
{% endblock %}